# Traefik Configuration
DOMAIN_SUFFIX=stack-ia.local
TRAEFIK_DASHBOARD_DOMAIN=traefik.stack-ia.local
ACME_EMAIL=your-email@example.com

# PostgreSQL Configuration
POSTGRES_USER=stackia
POSTGRES_PASSWORD=stackia_password_change_this
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=n8n_password

# n8n Configuration
N8N_ENCRYPTION_KEY=your-encryption-key-change-this-min-32-chars
N8N_DOMAIN=n8n.stack-ia.local
GENERIC_TIMEZONE=Europe/Paris

# Open WebUI Configuration
WEBUI_SECRET_KEY=your-secret-key-change-this-min-32-chars
WEBUI_AUTH=true
WEBUI_DOMAIN=webui.stack-ia.local

# Ollama Configuration
OLLAMA_HOST=0.0.0.0:11434
OLLAMA_DOMAIN=ollama.stack-ia.local

# AnythingLLM Configuration
LLM_PROVIDER=ollama
EMBEDDING_ENGINE=ollama
EMBEDDING_MODEL_PREF=nomic-embed-text
VECTOR_DB=qdrant
ANYTHINGLLM_DOMAIN=anythingllm.stack-ia.local

# Qdrant Configuration
QDRANT_DOMAIN=qdrant.stack-ia.local

# Adminer Configuration
ADMINER_DOMAIN=adminer.stack-ia.local

# Redis Configuration
REDIS_MAX_MEMORY=512mb

# SSL Configuration (optionnel - pour production)
ENABLE_SSL=false
LETS_ENCRYPT_STAGING=true

